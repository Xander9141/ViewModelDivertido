@model UsersViewModel

<!DOCTYPE html>
<html>
<head>
    <title>Users </title>
</head>
<body>
    <h1 style="text-align: center">Here are some Users!</h1>
    <div style="border: 1px solid black; padding: 10px; width: 50px; margin-left: 40px; padding: 120px;">
    <ul>
        @foreach (var user in Model.Users)
        {
            <li>@user.FirstName @user.LastName</li>
        }
    </ul>
</div>
</body>
</html>

